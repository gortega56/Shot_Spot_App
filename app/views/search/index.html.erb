<h1> <%= "Results For #{params[:query]}" %> </h1>

<%= render partial: "search" %>

<p>
    <h3> Categories </h3>
    <%= render partial: 'categories/categories' %>
  </p>

  <p>
      <h3> Nearby Photos </h3>
      <ul>
          <% @photosByAddress.each do |photo| %>
              <%= link_to image_tag(photo.image.url(:small)), photo_path(photo) %>
          <% end %>
      </ul>
  </p>
